{
  "name": "food-order-app",
  "version": "1.0.0",
  "description": "Full-stack MERN food ordering application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:start\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:start": "cd frontend && npm run build && npm run preview",
    "backend:install": "cd backend && npm install",
    "frontend:install": "cd frontend && npm install",
    "install:all": "npm install && npm run backend:install && npm run frontend:install",
    "backend:seed": "cd backend && npm run seed",
    "setup": "npm run install:all && npm run backend:seed"
  },
  "keywords": [
    "mern",
    "food-ordering",
    "react",
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
